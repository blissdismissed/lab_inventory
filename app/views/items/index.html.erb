<p>Sup Wu Tang</p>

  <table style="width:50%">
    <tr>
      <th>Item</th>
      <th>Quantity</th>
      <th>Room</th>
      <th>Location</th>
    </tr>
    <% @all_items.each do |f| %>
      <tr>
        <td><%= f.name %></td>
        <td><%= f.quantity %></td>
        <td><%= f.room_location %></td>
        <td><%= f.where_in_room %></td>
      </tr>
    <% end %>
  </table>