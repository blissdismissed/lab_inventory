<h3>Biomedical Engineering Lab Items</h3>
</br>
<table style="width:50%">
  <tr>
    <th>Item</th>
    <th>Quantity</th>
    <th>Room</th>
    <th>Location</th>
  </tr>
  <% @all_items.each do |f| %>
    <tr>
      <td><%= f.name %></td>
      <td><%= f.item_type %></td>
      <td><%= f.room_location %></td>
      <td><%= f.where_in_room %></td>
    </tr>
  <% end %>
</table>
</br>
<%= form_for(:items, url: items_new_path) do |g| %>
  <%= g.submit "Enter a new item", class: 'btn-primary' %>
<% end %>